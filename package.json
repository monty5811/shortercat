{
  "name": "shortercat",
  "version": "1.0.0",
  "repository": "git@gitlab.com:monty5811/shortercat.git",
  "author": "Dean Montgomery <montgomery.dean97@gmail.com>",
  "license": "GPL-3.0-only",
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@tailwindcss/custom-forms": "^0.2.1",
    "elm": "^0.19.0-bugfix2",
    "elm-format": "^0.8.1",
    "parcel-bundler": "^1.10.3",
    "postcss": "^7.0.21",
    "postcss-cli": "^6.1.3",
    "postcss-elm-tailwind": "^0.3.0",
    "postcss-modules": "^1.4.1",
    "prettier": "^1.14.3",
    "purgecss": "^1.4.1",
    "tailwindcss": "^1.1.3",
    "tailwindcss-gap": "^3.1.1"
  },
  "scripts": {
    "predev": "postcss -o main.css ./src/main.css",
    "dev": "parcel serve -p 4001 --hmr-port 9002 index.html",
    "prod": "./build.sh",
    "postprod": "touch dist/robots.txt && python sitemap.py && cp -R data dist/data",
    "sw": "workbox generateSW",
    "format:elm": "elm-format --yes src/",
    "ext": "rm -rf dist_ext && cp -R ext dist_ext && cp -R src/icons/* dist_ext"
  },
  "devDependencies": {
    "node-elm-compiler": "^5.0.1",
    "workbox-cli": "^3.6.3"
  }
}
